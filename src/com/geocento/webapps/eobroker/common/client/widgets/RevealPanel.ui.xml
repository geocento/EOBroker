<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:design="urn:import:gwt.material.design.client.ui"
             xmlns:iconmorph="urn:import:gwt.material.design.addins.client.iconmorph">
    <design:MaterialPanel>
        <design:MaterialPanel>
            <iconmorph:MaterialIconMorph display="INLINE_BLOCK" ui:field="switchIcon">
                <design:MaterialIcon iconType="ARROW_FORWARD" ui:field="forwardArrow"></design:MaterialIcon>
                <design:MaterialIcon iconType="ARROW_DOWNWARD" ui:field="downwardArrow"></design:MaterialIcon>
            </iconmorph:MaterialIconMorph>
            <design:MaterialLabel ui:field="title"></design:MaterialLabel>
        </design:MaterialPanel>
        <design:MaterialPanel ui:field="content">

        </design:MaterialPanel>
    </design:MaterialPanel>
</ui:UiBinder>