<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="eobroker">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="gwt.material.design.GwtMaterialWithJQuery" />
    <inherits name="gwt.material.design.addins.GwtMaterialAddins"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.geocento.webapps.eobroker.client.eobroker'/>

    <replace-with class="com.geocento.webapps.eobroker.client.ClientFactoryImpl">
        <when-type-is class="com.geocento.webapps.eobroker.client.ClientFactory"/>
    </replace-with>

    <source path='client'/>
    <source path='shared'/>

    <source path='common/client'/>

    <!-- Fallback User Agent for use by CRAWLERS -->
    <set-property-fallback name="user.agent" value="gecko1_8" />

</module>